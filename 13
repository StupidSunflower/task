using System;
class HelloWorld
{
    static void Main()
    {
        string text = "";
        int counter = 0;
        foreach (string word in text.ToLower().Split(""))
        {
            if (word.StartsWith("о"))
            {
                counter++;
            }
        }
        Console.WriteLine($"Количесто словв начинающихся с \"о":{ counter}")
    }
}
